{
  "name": "drawing-board",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "repository": "git@github.com:web-liuyang/drawing-board.git",
  "author": "web-liuyang <984584014@qq.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky && echo 'yarn lint-staged' > .husky/pre-commit",
    "start": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prettier": "prettier --write .",
    "eslint": "eslint --fix ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.ts": [
      "prettier --parset=typescript --write",
      "eslint --fix"
    ]
  }
}
